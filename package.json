    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.1.3",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recoil": "0.0.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@types/jest": "^26.0.14",
